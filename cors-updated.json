[
  {
    "corsRuleName": "cloudflow-frontend",
    "allowedOrigins": [
      "http://localhost:8080",
      "http://localhost:8081",
      "http://localhost:8082",
      "http://192.168.0.114:8080",
      "http://192.168.0.114:8081",
      "http://192.168.0.114:8082",
      "https://*.lovableproject.com",
      "https://cloudflow-vault.lovable.app",
      "https://loving-mclean-40d5c8.netlify.app"
    ],
    "allowedHeaders": [
      "*",
      "authorization",
      "content-type",
      "x-amz-date",
      "x-amz-user-agent",
      "x-amz-content-sha256",
      "x-amz-security-token",
      "cache-control",
      "expires"
    ],
    "allowedOperations": [
      "b2_download_file_by_id",
      "b2_download_file_by_name",
      "b2_upload_file",
      "b2_list_file_names",
      "b2_list_file_versions",
      "b2_delete_file_version",
      "b2_get_file_info",
      "b2_copy_file",
      "b2_update_file_legal_hold",
      "b2_update_file_retention"
    ],
    "exposeHeaders": [
      "x-bz-file-name",
      "x-bz-file-id",
      "x-bz-content-sha1",
      "etag"
    ],
    "maxAgeSeconds": 3600
  }
]
