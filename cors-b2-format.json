[
  {
    "corsRuleName": "cloudflow-frontend-complete",
    "allowedOrigins": [
      "http://localhost:8080",
      "http://localhost:8081", 
      "http://localhost:8082",
      "http://localhost:5173",
      "http://192.168.0.114:8080",
      "http://192.168.0.114:8081",
      "http://192.168.0.114:8082", 
      "http://192.168.0.114:5173",
      "https://*.lovableproject.com",
      "https://cloudflow-vault.lovable.app",
      "https://loving-mclean-40d5c8.netlify.app"
    ],
    "allowedHeaders": [
      "*",
      "authorization",
      "content-type",
      "x-amz-date",
      "x-amz-user-agent",
      "x-amz-content-sha256",
      "x-amz-security-token",
      "x-amz-request-id",
      "x-amz-id-2",
      "cache-control",
      "expires",
      "if-match",
      "if-none-match",
      "if-modified-since",
      "if-unmodified-since",
      "range",
      "x-bz-file-id",
      "x-bz-file-name",
      "x-bz-content-sha1",
      "x-bz-info-*"
    ],
    "allowedOperations": [
      "b2_download_file_by_id",
      "b2_download_file_by_name", 
      "b2_upload_file",
      "b2_list_file_names",
      "b2_list_file_versions",
      "b2_delete_file_version",
      "b2_get_file_info",
      "b2_copy_file",
      "b2_get_bucket",
      "b2_list_buckets",
      "b2_update_file_legal_hold",
      "b2_update_file_retention",
      "s3_get",
      "s3_put",
      "s3_delete",
      "s3_list"
    ],
    "exposeHeaders": [
      "x-bz-file-name",
      "x-bz-file-id", 
      "x-bz-content-sha1",
      "etag",
      "last-modified",
      "content-length",
      "content-type",
      "x-amz-request-id",
      "x-amz-id-2",
      "x-bz-info-*"
    ],
    "maxAgeSeconds": 3600
  }
]
